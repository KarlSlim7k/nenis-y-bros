{
  "version": 2,
  "name": "nenis-y-bros",
  "buildCommand": "",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/login",
      "destination": "/frontend/pages/auth/login.html"
    },
    {
      "source": "/registro",
      "destination": "/frontend/pages/auth/registro.html"
    },
    {
      "source": "/dashboard",
      "destination": "/frontend/pages/user/dashboard.html"
    },
    {
      "source": "/perfil",
      "destination": "/frontend/pages/user/perfil-empresarial.html"
    },
    {
      "source": "/diagnostico",
      "destination": "/frontend/pages/user/diagnosticos.html"
    },
    {
      "source": "/productos",
      "destination": "/frontend/pages/user/vitrina-productos.html"
    },
    {
      "source": "/logros",
      "destination": "/frontend/pages/user/mis-logros.html"
    },
    {
      "source": "/mentoria",
      "destination": "/frontend/pages/user/mentoria-ai.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
